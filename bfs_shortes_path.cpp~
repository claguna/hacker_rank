int main()
{
  int ntest;
  int n_nodes, n_edges;
  int start_node;
  vector< <int, int> > *adj_l;

  scanf("%d", &ntest);

  for (int i=0; i<ntest; i++){
    scanf("%d %d", &n_nodes, &n_edges);

    adj_l = new vector< <int, int> >()[n_nodes];

    for(int j=0; j< n_edges; j++){
      int x, y;
      scanf("%d %d",&x, &y);
      //add (x,y) to the adjancecy list

    }
    scanf("%d", &start_node);
  }
}
